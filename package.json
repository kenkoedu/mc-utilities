{
  "name": "mc-utilities",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bunx --bun vite\" \"bun run src/api/index.ts\"",
    "dev:frontend": "bunx --bun vite",
    "dev:backend": "bun run src/api/index.ts",
    "build": "vue-tsc && vite build && npm run build:backend",
    "build:backend": "bun run src/api/index.ts",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.15.5",
    "csv-parse": "^5.6.0",
    "elysia": "^1.2.25",
    "gh-pages": "^6.3.0",
    "lucide-vue-next": "^0.485.0",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-multiselect": "next",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.38.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/compiler-sfc": "^3.5.13",
    "bun-types": "^1.2.7",
    "concurrently": "^9.1.2",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "vite": "latest",
    "vue-tsc": "latest"
  },
  "homepage": "https://kenkoedu.github.io/mc-utilities/"
}